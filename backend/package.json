{
  "name": "personal-website-server",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/sendmail": "^1.4.2",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "sendmail": "^1.6.1",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "ts-node src/index.ts",
    "lint": "eslint --fix src/**/*.ts",
    "prettify": "prettier --write src/**/*{ts,tsx}"
  },
  "lint-staged": {
    "src/**/*.{ts,json,md}": [
      "npm run prettify",
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test -- --coverage"
    }
  }
}
